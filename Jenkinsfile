            }
        }
        stage('test') {
            steps {
                sh 'echo "Abhi project test ho raha hai..."'
            }
        }
        stage('deploy') {
            steps {
                sh 'echo "Abhi project deploy ho raha hai..."'
            }
        }
    }
}
